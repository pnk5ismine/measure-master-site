{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/data/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    },
    {
      "source": "/assets/(css|js)/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/images/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ],
  "redirects": [
    { "source": "https://www.measure-master.net/(.*)", "destination": "https://measure-master.net/$1", "permanent": true }
  ]
}
